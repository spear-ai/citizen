{
  "name": "@spear-ai/commitlint-config",
  "version": "2.1.0",
  "description": "Spear AI commitlint config",
  "author": {
    "name": "Spear AI",
    "email": "org@spear.ai",
    "url": "https://spear.ai"
  },
  "type": "module",
  "devDependencies": {
    "@commitlint/types": "18.4.3",
    "commitlint": "18.4.3",
    "eslint": "8.55.0",
    "npm-package-json-lint": "7.1.0",
    "prettier": "3.1.1",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vitest": "^1.2.0"
  },
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "peerDependencies": {
    "commitlint": "^18.4.3"
  },
  "repository": {
    "type": "git",
    "directory": "packages/commitlint-config",
    "url": "https://github.com/spear-ai/citizen.git"
  },
  "scripts": {
    "build": "tsup src --cjsInterop --clean --dts --format cjs,esm --splitting",
    "dev": "yarn build --watch & yarn test:dev",
    "eslint:check": "eslint --max-warnings 0 .",
    "eslint:fix": "yarn eslint:check --fix",
    "npmpkgjsonlint:check": "npmPkgJsonLint .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "vitest run",
    "test:dev": "vitest watch",
    "typescript:check": "tsc --noEmit"
  },
  "types": "./dist/index.d.ts"
}
