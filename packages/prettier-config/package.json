{
  "name": "@spear-ai/prettier-config",
  "version": "2.2.0",
  "description": "Spear AI Prettier config",
  "author": {
    "name": "Spear AI",
    "email": "org@spear.ai",
    "url": "https://spear.ai"
  },
  "type": "module",
  "dependencies": {
    "prettier-plugin-tailwindcss": "^0.6"
  },
  "devDependencies": {
    "@types/prettier": "3.0.0",
    "eslint": "8.57.0",
    "npm-package-json-lint": "7.1.0",
    "prettier": "3.5.3",
    "tsup": "8.0.2",
    "typescript": "5.4.5"
  },
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "peerDependencies": {
    "prettier": "^3"
  },
  "repository": {
    "type": "git",
    "directory": "packages/prettier-config",
    "url": "https://github.com/spear-ai/citizen.git"
  },
  "scripts": {
    "build": "tsup src --cjsInterop --clean --dts --format cjs,esm --splitting",
    "dev": "yarn build --watch",
    "eslint:check": "eslint --max-warnings 0 .",
    "eslint:fix": "yarn eslint:check --fix",
    "npmpkgjsonlint:check": "npmPkgJsonLint .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "typescript:check": "tsc --noEmit"
  },
  "types": "./dist/index.d.ts"
}
