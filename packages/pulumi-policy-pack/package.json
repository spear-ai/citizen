{
  "name": "@spear-ai/pulumi-policy-pack",
  "version": "2.0.0",
  "description": "Spear AI Pulumi Policy Pack",
  "author": {
    "name": "Spear AI",
    "email": "org@spear.ai",
    "url": "https://spear.ai"
  },
  "dependencies": {
    "@pulumi/awsguard": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "18.15.3",
    "eslint": "8.40.0",
    "npm-package-json-lint": "6.4.0",
    "prettier": "2.8.7",
    "tsup": "6.7.0",
    "typescript": "4.9.5"
  },
  "exports": {
    "./*": "./dist/*.cjs"
  },
  "license": "MIT",
  "repository": {
    "directory": "packages/pulumi-policy-pack",
    "type": "git",
    "url": "https://github.com/spear-ai/citizen.git"
  },
  "scripts": {
    "build": "tsup src --clean --dts --format cjs,esm",
    "dev": "yarn build --watch",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "prettier --write .",
    "lint:eslint": "eslint --max-warnings 0 .",
    "lint:npm": "npmPkgJsonLint .",
    "lint:prettier": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "update": "yarn upgrade-interactive"
  },
  "type": "module"
}
